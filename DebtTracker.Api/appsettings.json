{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DebtTracker": {
    "DAL": {
      "SqLite": {
        "Enabled": false,
        "DatabaseName": "DebtTracker.db",
        "SeedDemoData": true,
        "RecreateDatabaseEachTime": true
      },
      "SqlServer": {
        "Enabled": true,
        "ConnectionString": "Data Source=(localdb)\\MSSQLLocalDB;Initial Catalog=DebtTracker;Integrated Security=True;"
      }
    }
  }
}
